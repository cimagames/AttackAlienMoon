<?xml version="1.0" encoding="UTF-8"?>
<widget id="com.cima.attackalienmoon" version="1.0.0.0" android-versionCode="1000000" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
	<name>AttackAlienMoon</name>
	<description>AlienMoon</description>
	<author email="cimagames@hotmail.com" href="https://sites.google.com/view/cimagames/inicio"> cima</author>
	<content src="index.html" />
	<allow-intent href="http://*/*" />
	<allow-intent href="https://*/*" />
	<allow-navigation href="https://*/*" />
	<allow-navigation href="blob:*" />
	<preference name="orientation" value="default" />
	<preference name="fullscreen" value="false" />
	<preference name="DisallowOverscroll" value="true" />
	<preference name="LoadUrlTimeoutValue" value="120000" />
	<access origin="*" />
	<preference name="AutoHideSplashScreen" value="false" />
	<preference name="ShowSplashScreenSpinner" value="false" />
	<preference name="InspectableWebview" value="true" />
	<engine name="android" spec="12.0.1" />
	<platform name="android">
		<preference name="android-minSdkVersion" value="24" />
		<preference name="android-targetSdkVersion" value="33" />
		<preference name="AndroidWindowSplashScreenBackground" value="#ffffff" />
	</platform>
</widget>
